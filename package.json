{
  "name": "bitmark",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "watchman watch-del-all && rm -rf node_modules/ && rm -rf ios/build/ && rm -rf android/app/build/ && npm cache clean --force && npm install",
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "a-test": "react-native run-android --variant=inhouseDebug",
    "a-live": "cd android && ./gradlew fillSecretKey && cd .. && react-native run-android --variant=prdDebug",
    "i-test": "react-native run-ios --scheme 'Bitmark Registry dev'",
    "i-test-x": "react-native run-ios --scheme 'Bitmark Registry dev' --simulator='iPhone X'",
    "i-live": "react-native run-ios",
    "test": "npm run test-delete-simulator-photos && jest --no-cache --runInBand --config=jest.config.json",
    "test-kill-all": "killall node || true",
    "test-build": "npm run start & npm run i-test",
    "test-execute": "appium & npm run test",
    "test-delete-simulator-photos": " node __tests__/utils/delete-simulator-photos.js",
    "test-jenkins": "npm run test-kill-all && npm run clean && npm run test-build && sleep 60 && npm run test-execute && sleep 60 && npm run test-kill-all "
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bs58": "^4.0.1",
    "buffer": "^5.2.1",
    "i18n-js": "^3.0.11",
    "js-sha3": "^0.8.0",
    "lodash": "^4.17.4",
    "lottie-react-native": "^2.5.11",
    "moment": "^2.20.1",
    "prop-types": "^15.6.2",
    "random-string": "^0.2.0",
    "react": "16.8.3",
    "react-native": "0.59.2",
    "react-native-actionsheet": "^2.4.2",
    "react-native-camera": "^0.13.0",
    "react-native-code-push": "^5.3.2",
    "react-native-cookies": "^3.3.0",
    "react-native-device-info": "^0.26.4",
    "react-native-document-picker": "^2.2.0",
    "react-native-fs": "^2.13.3",
    "react-native-highlight-words": "^1.0.1",
    "react-native-hyperlink": "0.0.14",
    "react-native-image-picker": "^0.28.0",
    "react-native-intercom": "^12.5.1",
    "react-native-keep-awake": "^3.0.1",
    "react-native-mail": "^3.0.5",
    "react-native-mime-types": "^2.2.1",
    "react-native-push-notification": "^3.0.2",
    "react-native-qrcode": "^0.2.7",
    "react-native-qrcode-svg": "^5.1.2",
    "react-native-router-flux": "^4.0.5",
    "react-native-sentry": "^0.40.2",
    "react-native-share": "^1.1.3",
    "react-native-svg": "^9.3.6",
    "react-native-swiper": "^1.5.4",
    "react-native-video": "^4.3.1",
    "react-native-zip-archive": "^3.0.1",
    "react-navigation": "^2.18.3",
    "react-navigation-tabs": "^0.8.4",
    "react-redux": "^6.0.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "babel-eslint": "^9.0.0",
    "babel-jest": "^24.1.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "eslint": "^5.4.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-native": "^3.2.1",
    "jest": "^24.1.0",
    "jest-junit-reporter": "^1.1.0",
    "metro-react-native-babel-preset": "^0.52.0",
    "node-simctl": "^4.0.2",
    "react-test-renderer": "16.6.1",
    "sharp": "^0.22.0",
    "wd": "^1.11.1"
  }
}
